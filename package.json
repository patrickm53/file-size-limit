{
  "name": "size-limit",
  "version": "0.21.0",
  "description": "Prevent JS libraries bloat. If you accidentally add a massive dependency, Size Limit will throw an error.",
  "keywords": [
    "size",
    "limit",
    "travis",
    "ci",
    "bundle",
    "webpack"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/size-limit",
  "bin": "cli.js",
  "engines": {
    "node": ">=6.11.5"
  },
  "dependencies": {
    "bytes": "^3.0.0",
    "chalk": "^2.4.1",
    "ci-job-number": "^0.3.0",
    "compression-webpack-plugin": "^2.0.0",
    "cosmiconfig": "^5.0.6",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "escape-string-regexp": "^1.0.5",
    "file-loader": "^2.0.0",
    "globby": "^8.0.1",
    "gzip-size": "^5.0.0",
    "memory-fs": "^0.4.1",
    "postcss-loader": "^3.0.0",
    "read-pkg-up": "^4.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.1.2",
    "clean-publish": "^1.0.10",
    "cross-spawn": "^6.0.5",
    "eslint": "^5.8.0",
    "eslint-ci": "^0.1.1",
    "eslint-config-logux": "^26.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.26.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.3",
    "redux": "^4.0.1",
    "yaspeller-ci": "^1.0.0"
  },
  "scripts": {
    "spellcheck": "yaspeller-ci *.md",
    "lint": "eslint-ci *.js test/{fixtures/**/,}*.js",
    "lint-fix": "eslint --fix *.js test/{fixtures/**/,}*.js",
    "test": "jest --forceExit && yarn run lint && yarn run spellcheck"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/test/fixtures"
    ]
  },
  "eslintConfig": {
    "extends": "eslint-config-logux/node",
    "rules": {
      "security/detect-non-literal-require": "off",
      "security/detect-non-literal-regexp": "off",
      "global-require": "off"
    }
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "dictionary": [
      "Versioning",
      "Travis",
      "CI",
      "JS",
      "Webpack",
      "CLI",
      "Marais",
      "Rossouw",
      "bundlesize",
      "Yaroslav",
      "Markin",
      "Autoprefixer",
      "PostCSS",
      "Logux",
      "webpack",
      "polyfill",
      "js",
      "MobX",
      "gzip",
      "UI",
      "ES2016",
      "Babili",
      "minifier",
      "npm",
      "deHaan",
      "Alexandr",
      "Subbotin",
      "Werner",
      "Browserslist",
      "nanoid",
      "EmojiMart",
      "Marinin",
      "Sivan",
      "Mehta",
      "Borovik",
      "Jayden",
      "Seric",
      "Bogdan",
      "Chadkin",
      "Ruf",
      "Olivier",
      "Tassinari",
      "webpackâ€™s",
      "polyfills",
      "CircleCI",
      "UglifyJS",
      "in CI",
      "Naijia",
      "Liu"
    ]
  }
}
