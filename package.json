{
  "name": "size-limit",
  "version": "0.11.4",
  "description": "Prevent JS libraries bloat. If you accidentally add a massive dependency, Size Limit will throw an error.",
  "keywords": [
    "size",
    "limit",
    "travis",
    "ci",
    "bundle",
    "webpack"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/size-limit",
  "bin": "cli.js",
  "dependencies": {
    "bytes": "^3.0.0",
    "chalk": "^2.1.0",
    "ci-job-number": "^0.3.0",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.7",
    "escape-string-regexp": "^1.0.5",
    "file-loader": "^0.11.2",
    "globby": "^6.1.0",
    "gzip-size": "^4.0.0",
    "memory-fs": "^0.4.1",
    "read-pkg-up": "^2.0.0",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "yargs": "^9.0.1"
  },
  "devDependencies": {
    "attr-accept": "^1.1.0",
    "cross-spawn": "^5.1.0",
    "eslint": "^4.7.2",
    "eslint-config-logux": "^16.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "pre-commit": "^1.2.2",
    "redux": "^3.7.2",
    "yaspeller-ci": "^0.7.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yaspeller-ci *.md",
    "lint": "eslint *.js test/{fixtures/**/,}*.js",
    "test": "jest && yarn run lint && yarn run spellcheck"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-logux/node4",
    "globals": {
      "jasmine": true
    }
  },
  "lint-staged": {
    "*.md": "yaspeller-ci",
    "*.js": "eslint"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
